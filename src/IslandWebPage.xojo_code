	#tag Method, Flags = &h21
		Private Sub CreateNewRoomHandler(sender As WebContainer, x As Integer, y As Integer)
		  mNewRoomPosition = x : y
		  
		  Var newRoom As New MUD.Room
		  newRoom.Name = x.ToString + "," + y.ToString
		  newRoom.X = mNewRoomPosition.Left
		  newRoom.Y = mNewRoomPosition.Right
		  newRoom.Z = mZ
		  newRoom.Zone = Island.Zones(mCurrentZoneIndex)
		  Island.Zones(mCurrentZoneIndex).Rooms.Add(newRoom)
		  
		  For i As Integer = 0 To mEmptyRoomContainers.LastIndex
		    If sender = mEmptyRoomContainers(i) Then
		      mEmptyRoomContainers.RemoveAt(i)
		      Exit
		    End If
		  Next
		  sender.Close
		  
		  Refresh
		  
		  Session.SaveMap
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub DrawRoomContainers()
		  Const defaultValue = -9999
		  Var grid As New Dictionary
		  
		  If mCurrentZoneIndex > Island.Zones.LastIndex Then
		    Return
		  End If
		  
		  If mCurrentZoneIndex < 0 Or mCurrentZoneIndex > Island.Zones.LastIndex Then
		    Return
		  End If
		  
		  Var zone As MUD.Zone = Island.Zones(mCurrentZoneIndex)
		  Var rooms() As MUD.Room
		  For Each currentZone As MUD.Zone In Island.Zones
		    For Each room As MUD.Room In currentZone.Rooms
		      If room.Z = mZ Then
		        rooms.Add(room)
		      End If
		    Next
		  Next
		  
		  Var testContainer As New IslandRoomContainer
		  If rooms.Count = 0 Then
		    For Each c As WebContainer In mRoomContainers
		      c.Close
		    Next
		    
		    For Each c As WebContainer In mEmptyRoomContainers
		      c.Close
		    Next
		    
		    mRoomContainers.RemoveAll
		    mEmptyRoomContainers.RemoveAll
		    
		    RoomsRectangle.Width = testContainer.Width
		    RoomsRectangle.Height = testContainer.Height
		    Var c As New IslandAddRoomContainer
		    c.X = 0
		    c.Y = 0
		    AddHandler c.CreateRoom, WeakAddressOf CreateNewRoomHandler
		    c.EmbedWithin(RoomsRectangle, RoomsRectangle.Left, RoomsRectangle.Top, c.Width, c.Height)
		    mEmptyRoomContainers.Add(c)
		    RoomsRectangle.Left = Session.ClientWidth / 2 - RoomsRectangle.Width / 2
		    RoomsRectangle.Top = Max(MainToolbar.Height, Session.ClientHeight / 2 - RoomsRectangle.Height / 2)
		    Return
		  End If
		  
		  Var padding As Integer = 0
		  Var minX, minY, maxX, maxY As Integer = defaultValue
		  
		  For Each room As MUD.Room In rooms
		    minX = If(minX = defaultValue, room.X, Min(minX, room.X))
		    minY = If(minY = defaultValue, room.Y, Min(minY, room.Y))
		    maxX = If(maxX = defaultValue, room.X, Max(maxX, room.X))
		    maxY = If(maxY = defaultValue, room.Y, Max(maxY, room.Y))
		  Next
		  
		  minX = minX - 1
		  maxX = maxX + 1
		  minY = minY - 1
		  maxY = maxY + 1
		  
		  Var diffX As Integer = maxX - minX + 1
		  Var diffY As Integer = maxY - minY + 1
		  
		  RoomsRectangle.Width = Min(Session.ClientWidth, diffX * testContainer.Width)
		  RoomsRectangle.Height = Min(Session.ClientHeight - MainToolbar.Height, diffY * testContainer.Height)
		  
		  Var offsetX As Integer = (0 - minX) * testContainer.Width
		  Var offsetY As Integer = (0 - minY) * testContainer.Height
		  
		  testContainer.Close
		  
		  For i As Integer = 0 To rooms.LastIndex
		    Var room As MUD.Room = rooms(i)
		    Var c As IslandRoomContainer
		    If i <= mRoomContainers.LastIndex Then
		      c = mRoomContainers(i)
		      c.Left = room.X * c.Width + offsetX
		      c.Top = room.Y * c.Height + offsetY
		    Else
		      c = New IslandRoomContainer
		      AddHandler c.Pressed, WeakAddressOf RoomPressedHandler
		      c.EmbedWithin(RoomsRectangle, RoomsRectangle.Left + room.X * c.Width + offsetX, RoomsRectangle.Top + room.Y * c.Height + offsetY, c.Width, c.Height)
		      mRoomContainers.Add(c)
		    End If
		    
		    c.Room = room
		    c.HasLadder = Island.RoomHasLadders(room)
		    SetBorders(c)
		    grid.Value(room.X.ToString + "-" + room.Y.ToString) = True
		  Next
		  
		  While mRoomContainers.LastIndex > rooms.LastIndex
		    mRoomContainers.RemoveAt(mRoomContainers.LastIndex)
		  Wend
		  
		  For i As Integer = 0 To mEmptyRoomContainers.LastIndex
		    Var c As IslandAddRoomContainer = mEmptyRoomContainers(i)
		    grid.Value(c.X.ToString + "-" + c.Y.ToString) = True
		  Next
		  
		  For Each room As MUD.Room In rooms
		    Var positions() As Pair = Array(room.X : room.Y - 1, room.X : room.Y + 1, room.X + 1 : room.Y, room.X - 1 : room.Y, _
		    room.X - 1 : room.Y - 1, room.X + 1 : room.Y - 1, room.X - 1 : room.Y + 1, room.X + 1 : room.Y + 1)
		    For Each position As Pair In positions
		      Var key As String = position.Left.StringValue + "-" + position.Right.StringValue
		      If Not grid.HasKey(key) Then
		        Var c As New IslandAddRoomContainer
		        c.X = position.Left
		        c.Y = position.Right
		        AddHandler c.CreateRoom, WeakAddressOf CreateNewRoomHandler
		        c.EmbedWithin(RoomsRectangle, RoomsRectangle.Left + position.Left * c.Width + offsetX, RoomsRectangle.Top + position.Right * c.Height + offsetY, c.Width, c.Height)
		        mEmptyRoomContainers.Add(c)
		        grid.Value(key) = True
		      End If
		    Next
		  Next
		  
		  For i As Integer = mEmptyRoomContainers.LastIndex DownTo 0
		    Var emptyRoom As IslandAddRoomContainer = mEmptyRoomContainers(i)
		    emptyRoom.Left = emptyRoom.X * emptyRoom.Width + offsetX
		    emptyRoom.Top = emptyRoom.Y * emptyRoom.Height + offsetY
		    
		    If Not grid.HasKey(emptyRoom.X.ToString + "-" + emptyRoom.Y.ToString) Then
		      mEmptyRoomContainers(i).Close
		      mEmptyRoomContainers.RemoveAt(i)
		    End If
		  Next
		  
		  RoomsRectangle.Left = Session.ClientWidth / 2 - RoomsRectangle.Width / 2
		  RoomsRectangle.Top = Max(MainToolbar.Height, Session.ClientHeight / 2 - RoomsRectangle.Height / 2)
		  RoomsRectangle.LockLeft = False
		  RoomsRectangle.LockTop = False
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub GoToMap()
		  Self.Close
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub Refresh(fullRefresh As Boolean = False)
		  If fullRefresh Then
		    mEmptyRoomContainers.RemoveAll
		    mRoomContainers.RemoveAll
		    For Each ctl As WebUIControl In Controls
		      If ctl IsA IslandAddRoomContainer Or ctl IsA IslandRoomContainer Then
		        ctl.Close
		      End If
		    Next
		    
		    ExecuteJavaScript("document.getElementById('" + RoomsRectangle.ControlID + "').scrollTo(0, 0)")
		  End If
		  
		  Var zonesButton As WebToolbarItem = MainToolbar.ItemWithTag("zone")
		  If zonesButton <> Nil And Island <> Nil Then
		    zonesButton.Caption = "Zones (" + Island.Zones.Count.ToString + ")"
		  End If
		  
		  Var b As WebToolbarButton = WebToolbarButton(MainToolbar.ItemAt(2))
		  Var menu As New WebMenuItem
		  b.Menu = menu
		  If Island = Nil Then
		    Return
		  End If
		  
		  For i As Integer = 0 To Island.Zones.LastIndex
		    Var zone As MUD.Zone = Island.Zones(i)
		    menu.AddMenuItem(New WebMenuItem(zone.Name, zone.Id))
		    menu.MenuItemAt(menu.LastAddedIndex).Tag = i
		  Next
		  
		  TitleLabel.Text = Island.Name + If(mCurrentZoneIndex >= 0 And mCurrentZoneIndex <= island.Zones.LastIndex, " - " + Island.Zones(mCurrentZoneIndex).Name, "")
		  
		  If mCurrentZoneIndex >= 0 And mCurrentZoneIndex <= Island.Zones.LastIndex Then
		    Var zRange As Pair = Island.Zones(mCurrentZoneIndex).ZRange
		    ZSlider.MinimumValue = zRange.Left
		    ZSlider.MaximumValue = zRange.Right
		    ZSlider.Visible = ZSlider.MinimumValue <> ZSlider.MaximumValue
		  End If
		  
		  DrawRoomContainers
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub RoomPressedHandler(sender As IslandRoomContainer, x As Integer, y As Integer)
		  EditRoomDialog.Room = sender.Room
		  EditRoomDialog.Show
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SetBorders(c As IslandRoomContainer)
		  Var room As MUD.Room = c.Room
		  
		  Var x As Integer = room.X
		  Var y As Integer = room.Y
		  
		  Var north As Boolean = Island.RoomExists(x, y - 1, mZ)
		  Var south As Boolean = Island.RoomExists(x, y + 1, mZ)
		  Var east As Boolean = Island.RoomExists(x + 1, y, mZ)
		  Var west As Boolean = Island.RoomExists(x - 1, y, mZ)
		  
		  c.SetBorders(north, south, east, west)
		End Sub
	#tag EndMethod


	#tag ComputedProperty, Flags = &h0
		#tag Getter
			Get
			  Return mIsland
			End Get
		#tag EndGetter
		#tag Setter
			Set
			  mIsland = value
			  Title = value.Name
			  mCurrentZoneIndex = If(value = Nil Or value.Zones.Count = 0, -1, 0)
			  Refresh(True)
			End Set
		#tag EndSetter
		Island As MUD.Island
	#tag EndComputedProperty

