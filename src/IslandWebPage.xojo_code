#tag Events MainToolbar
	#tag Event
		Sub Opening()
		  Me.Icon = modernmudlogo
		  
		  Me.AddItem(New WebToolbarButton("Map"))
		  Me.ItemAt(Me.LastItemIndex).Tag = "map"
		  
		  Var create As New WebToolbarButton("Create")
		  create.Style = WebToolbarButton.ButtonStyles.Menu
		  Var createMenu As New WebMenuItem
		  createMenu.AddMenuItem(New WebMenuItem("Zone", "create_zone"))
		  create.Menu = createMenu
		  Me.AddItem(create)
		  Me.ItemAt(Me.LastItemIndex).Tag = "create"
		  
		  Me.AddItem(New WebToolbarButton("Zones"))
		  WebToolbarButton(Me.ItemAt(Me.LastItemIndex)).Style = WebToolbarButton.ButtonStyles.Menu
		  Me.ItemAt(Me.LastItemIndex).Tag = "zone"
		End Sub
	#tag EndEvent
	#tag Event
		Sub Pressed(item As WebToolbarButton)
		  Select Case item.Tag
		  Case "map"
		    GoToMap
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Sub TitlePressed()
		  GoToMap
		End Sub
	#tag EndEvent
	#tag Event
		Sub MenuSelected(item As WebToolbarButton, hitItem As WebMenuItem)
		  Select Case item.Tag
		  Case "create"
		    Select Case hitItem.Tag
		    Case "create_zone"
		      CreateZoneNameWebDialog.Show
		    End Select
		    
		  Case "zone"
		    If mCurrentZoneIndex <> hitItem.Tag Then
		      mCurrentZoneIndex = hitItem.Tag
		      Refresh(True)
		    End If
		  End Select
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CreateZoneNameWebDialog
	#tag Event
		Sub OkPressed(value As String)
		  Var newZone As New MUD.Zone
		  newZone.Id = value.Lowercase.ReplaceAll(" ", "_")
		  newZone.Name = value
		  newZone.Island = Island
		  
		  Island.Zones.Add(newZone)
		  mCurrentZoneIndex = Island.Zones.LastIndex
		  Refresh(True)
		  
		  Session.SaveMap
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CreateRoomWebDialog
	#tag Event
		Sub OkPressed(value As String)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events EditRoomDialog
	#tag Event
		Sub SavePressed()
		  Refresh
		  
		  Session.SaveMap
		End Sub
	#tag EndEvent
	#tag Event
		Sub DeleteRoomPressed()
		  Me.Room.Zone.RemoveRoomAt(Me.Room.X, Me.Room.Y, Me.Room.Z)
		  
		  For i As Integer = 0 To mRoomContainers.LastIndex
		    If Me.Room = mRoomContainers(i).Room Then
		      mRoomContainers(i).Close
		      mRoomContainers.RemoveAt(i)
		      Exit
		    End If
		  Next
		  
		  Refresh
		  
		  Session.SaveMap
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ZSlider
	#tag Event
		Sub ValueChanged()
		  mZ = Me.Value
		  RoomsRectangle.Width = 100
		  RoomsRectangle.Height = 100
		  RoomsRectangle.Top = 20
		  RoomsRectangle.Left = 20
		  Refresh(True)
		End Sub
	#tag EndEvent
